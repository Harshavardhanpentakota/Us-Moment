
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  email String @unique
  firstName String?
  lastName String?
  password String 
}

model Saved {
  id Int @id 
  SectionName String 
  TopicName String
}

model Completed {
  id Int @id
  SectionName String
  TopicName String
}